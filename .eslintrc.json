{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": ["airbnb-base"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 13
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"import/prefer-default-export": "off"
	}
}
